er=()=>{\n        navigate('/login?redirect=/shipping');\n    }\n  return (\n    \n     <Row>\n        <Col md= {8}>\n            <h1 style = {{marginBottom:'20px'}}>Shopping Cart</h1>\n             {cartItems.length===0?(\n                <Message>\n                    Your cart is empty<Link to = '/'>Go Back</Link>\n\n                </Message>\n             ):\n             (<ListGroup varient = 'flush'>\n               {cartItems.map((items)=>(\n                <ListGroup.Item key ={items.id}>\n                    <Row>\n                        <Col md={2}>\n                            <Image src = {items.image} alt ={items.name} fluid rounded/>\n                        </Col>\n                        <Col md={3}>\n                            <Link to = {`/product/${items._id}`}>{items.name}</Link>\n                        </Col>\n                        <Col md ={2}>\n                            ${items.price}\n                        </Col>\n                        <Col md={2}>\n                        <Form.Control as='select' value={items.qty} onChange={(e)=>addToCartHandler(items,Number(e.target.value))}>\n                        {[...Array(items.countInStock).keys()].map((x)=>(\n                            <option key ={x+1} value={x+1}>\n                      {x+1}\n                    </option>\n                  ))}\n                </Form.Control>\n                        </Col>\n                        <Col md={2}>\n                            <Button type='button' varient ='light' onClick={()=>removeFromCartHandler(items._id)}>\n                                <FaTrash/>\n                            </Button>\n                        </Col>\n                    </Row>\n                </ListGroup.Item>\n               ))}\n             </ListGroup>)}\n        </Col>\n        <Col md={4}>\n            <Card>\n                <ListGroup varient = 'flush'>\n                    <ListGroup.Item>\n                        <h2>SubTotal({cartItems.reduce((acc